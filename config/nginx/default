upstream authentic {
    server authentic:8080;
}
server {
        listen   80;

        server_name agents.waco.localhost;

        location ~ ^/static/(.+)$ {
            root /;
            try_files /var/lib/authentic2-multitenant/static/$1
                      /var/lib/authentic2-multitenant/collectstatic/$1
                      =404;
        }

        location / {
            rewrite            ^/accounts/saml/login/ /accounts/fedict/login/ last;
            # include            uwsgi_params;
            # uwsgi_pass         authentic;
            proxy_pass         http://authentic;
            proxy_set_header   Host $http_host;
            proxy_set_header   X-Real-IP       $remote_addr;
            proxy_set_header   X-Forwarded-For $proxy_add_x_forwarded_for;
            # proxy_set_header   X-Forwarded-SSL on;
            # proxy_set_header   X-Forwarded-Protocol ssl;
            # proxy_set_header   X-Forwarded-Proto https;
        }
        error_log /var/log/nginx/error-agents.waco.localhost.log warn;
        access_log /var/log/nginx/access-agents.waco.localhost.log combined;
}

server {
        listen   80;

        server_name usagers.waco.localhost;

        location ~ ^/static/(.+)$ {
            root /;
            try_files /var/lib/authentic2-multitenant/static/$1
                      /var/lib/authentic2-multitenant/collectstatic/$1
                      =404;
        }

        location / {
            rewrite            ^/accounts/saml/login/ /accounts/fedict/login/ last;
            proxy_pass         http://authentic;
            proxy_set_header   Host $http_host;
            proxy_set_header   X-Real-IP       $remote_addr;
            proxy_set_header   X-Forwarded-For $proxy_add_x_forwarded_for;
            # proxy_set_header   X-Forwarded-SSL on;
            # proxy_set_header   X-Forwarded-Protocol ssl;
            # proxy_set_header   X-Forwarded-Proto https;
        }
        error_log /var/log/nginx/error-usagers.waco.localhost.log warn;
        access_log /var/log/nginx/access-usagers.waco.localhost.log combined;
}
